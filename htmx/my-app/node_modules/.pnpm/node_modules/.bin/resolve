#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/david.johansson/kod/lab/next-auth/my-authed-htmx-app/node_modules/.pnpm/node_modules/resolve/bin/node_modules:/Users/david.johansson/kod/lab/next-auth/my-authed-htmx-app/node_modules/.pnpm/node_modules/resolve/node_modules:/Users/david.johansson/kod/lab/next-auth/my-authed-htmx-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/david.johansson/kod/lab/next-auth/my-authed-htmx-app/node_modules/.pnpm/node_modules/resolve/bin/node_modules:/Users/david.johansson/kod/lab/next-auth/my-authed-htmx-app/node_modules/.pnpm/node_modules/resolve/node_modules:/Users/david.johansson/kod/lab/next-auth/my-authed-htmx-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../resolve/bin/resolve" "$@"
else
  exec node  "$basedir/../resolve/bin/resolve" "$@"
fi
